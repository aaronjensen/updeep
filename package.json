    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "microbundle": "^0.11.0",
    "mocha": "^7.0.0",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3",
    "webpack": "^4.42.1",
